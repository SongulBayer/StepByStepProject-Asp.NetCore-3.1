                                <a class="dropdown-item" asp-area="" asp-controller="Kategori" asp-action="Index">Kategoriler</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Film" asp-action="Index">Filimler</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-area="" asp-controller="Country" asp-action="Index">Ülkeler</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Actor" asp-action="Index">Oyuncular</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Yonetmen" asp-action="Index">Yonetmenler</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Il" asp-action="Index">İller</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Ilce" asp-action="Index">İlçeler</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Sinema" asp-action="Index">Sinamalar</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Salon" asp-action="Index">Salonlar</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Gosterimler" asp-action="Index">Gösterimler</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Seans" asp-action="Index">Seanslar</a>
                                <a class="dropdown-item" asp-area="" asp-controller="FilmAktor" asp-action="Index">Film ve Oyuncular</a>
                                <a class="dropdown-item" asp-area="" asp-controller="YonetmenFılm" asp-action="Index">Film ve Yönetmenler</a>

                            </div>
                        </li>
                        @*<li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" id="navbarDropdownPortfolio" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">Portfolio</a>
                            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownPortfolio">
                                <li><a class="dropdown-item" href="portfolio-overview.html">Portfolio Overview</a></li>
                                <li><a class="dropdown-item" href="portfolio-item.html">Portfolio Item</a></li>
                            </ul>
                        </li>*@
                    </ul>
                </div>
            </div>
        </nav>
    </header>


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-secondary bg-primary">
        <div class="container">
            &copy; 2022 - StepByStepProject - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)


</body>
</html>
